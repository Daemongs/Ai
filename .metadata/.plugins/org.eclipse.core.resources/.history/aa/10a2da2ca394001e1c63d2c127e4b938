package ch05.sec03;

public class 이차원배열 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[][] intArr1 = { {1,2},{2,3},{3,4}}; //0행,1행,2행,
		String[][] strArr1 = {{"a","b","c"},{"d","e","f"}};
		boolean[][] bArr1 = { {false,true},{false,true},{false,true},{false,true}};
		
		//2차원 배열의 값을 출력하기
		System.out.println(intArr1[2][1]);
		System.out.println(strArr1[0][1]);
	}

}
