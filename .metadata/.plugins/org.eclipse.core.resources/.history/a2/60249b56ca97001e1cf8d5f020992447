package ch06.sec06;

public class Car {

	String company = "현대자동차";
	String model = "그랜저";
	String color = "검정";
	int maxSpeed = 350;
	boolean start;
	int speed;
	
	public Car() {
	this();	
	}
	
	
	
	public Car(String company, String model, String color, int maxSpeed, intspeed) {
		
		super();
		this.company = company;
		this.model = model;
		this.color=color;
		this.maxSpeed = maxSpeed;
		this.speed=speed;
	}
	
	
}
	
	
