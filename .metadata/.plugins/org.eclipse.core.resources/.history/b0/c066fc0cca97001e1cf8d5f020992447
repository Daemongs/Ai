package ch06.sec06;

public class Car {

	String company = "현대자동차";
	String model = "그랜저";
	String color = "검정";
	int maxSpeed = 350;
	boolean start;
	int speed;
	
	public Car() {
		
	}
	
	
	public Car(String string, String string2, String string3, int i, int j) {
		
		super();
		this.company = company;
		this.model = model;
		this.color.color;
		this.maxSpeed = maxSpeed;
		this.speed=speed;
	}
	
	
}
	
	
